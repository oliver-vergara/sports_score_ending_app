<% if current_user %>
  <h2 class="active"> Active Games </h2>

  <div class="row">
    <% @games.each do |game| %>
    <div class="col-md-3">
      <div class="thumbnail">
        <a href="/games/<%= game.id %>">
          <p><%= game.schedule %></p>
          <p><%= game.name %></p>
        </a>
      </div>
    </div>
    <% end %>
  </div>
        
<% else %>

<%  end %>